n = int(input("Enter a number: "))

# Check if even or odd
if n % 2 == 0:
    print("The number is even.")
else:
    print("The number is odd.")

# Check if prime
if n > 1:
    for i in range(2, n):
        if n % i == 0:
            print("The number is not prime.")
            break
    else:
        print("The number is prime.")
else:
    print("The number is not prime.")
